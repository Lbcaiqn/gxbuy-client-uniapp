{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "MySearcher": "@/components/common/MySearcher.vue",
      "UserAddress": "@/components/common/UserAddress.vue",
      "PullUpLoading": "@/components/common/PullUpLoading.vue",
      "GoodsCommentBox": "@/components/common/GoodsCommentBox.vue",
      "ShopInfo": "@/components/common/ShopInfo.vue",
